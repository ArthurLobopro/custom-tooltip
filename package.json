{
  "name": "@arthur-lobo/custom-tooltip",
  "version": "1.0.0",
  "author": "Arthur Lobo",
  "license": "MIT",
  "description": "A css package to use custom tooltips on your site",
  "keywords": ["tooltip", "custom", "css"],
  "scripts": {
    "build": "node scripts/build.js",
    "prepack": "yarn build"
  },
  "devDependencies": {
    "@types/node": "^20.14.8",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "postcss-scss": "^4.0.9",
    "sass": "^1.77.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.css",
  "files": ["./dist/*.css"],
  "exports": {
    ".": {
      "require": "./dist/index.css",
      "import": "./dist/index.css"
    }
  }
}
